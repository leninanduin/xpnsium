{
    "manifest_version": 2,

    "name": "Xpnsium",
    "description": "Track online shopping expenses.",
    "version": "0.1",

    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["xpnsium.js"]
        }
    ],
    "permissions": [
        "history",
        "storage"
    ]
